@font-face {
	font-family: 'audiocancun';
	src:url(<%= asset_path 'audiocancun.eot?-qy2654' %>);
	src:url(<%= asset_path 'audiocancun.eot?#iefix-qy2654' %>) format('embedded-opentype'),
		url(<%= asset_path 'audiocancun.woff?-qy2654' %>) format('woff'),
		url(<%= asset_path 'audiocancun.ttf?-qy2654' %>) format('truetype'),
		url(<%= asset_path 'audiocancun.svg?-qy2654#audiocancun' %>) format('svg');
	font-weight: normal;
	font-style: normal;
}

[class^="icon-"], [class*=" icon-"] {
	font-family: 'audiocancun';
	speak: none;
	font-style: normal;
	font-weight: normal;
	font-variant: normal;
	text-transform: none;
	line-height: 1;

	/* Better Font Rendering =========== */
	-webkit-font-smoothing: antialiased;
	-moz-osx-font-smoothing: grayscale;
}

.icon-phone:before {
	content: "\e600";
}
.icon-mail:before {
	content: "\e601";
}
.icon-location:before {
	content: "\e602";
}
.icon-comment:before {
	content: "\e603";
}
.icon-music:before {
	content: "\e604";
}
.icon-share:before {
	content: "\e605";
}
.icon-facebook:before {
	content: "\e606";
}
.icon-youtube:before {
	content: "\e607";
}
